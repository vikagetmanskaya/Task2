<?xml version="1.0" encoding="UTF-8" ?>
<flowers xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.example.com/flowers flowers.xsd">

    <flower id="1" name="chamomile">
        <soil>ground</soil>
        <origin>Altai</origin>
        <visual-parametrs>
            <steam-color>green</steam-color>
            <leaf-color>white</leaf-color>
            <size>0.2 m</size>
        </visual-parametrs>
        <multiplying>seeds</multiplying>
        <date-of-planting>2021-05-15</date-of-planting>
        <growing-tips>
            <temperature>20 degree</temperature>
            <lighting>light-loving</lighting>
            <watering>200 ml</watering>
        </growing-tips>
    </flower>
    <tree id="2" name="eucalyptus" description="evergreen tree">
        <soil>ground</soil>
        <origin>Australia</origin>
        <visual-parametrs>
            <steam-color>brown</steam-color>
            <leaf-color>green</leaf-color>
            <size>30 m</size>
        </visual-parametrs>
        <multiplying>cuttings</multiplying>
        <date-of-planting>2021-03-20</date-of-planting>
        <growing-tips>
            <temperature>15 degree</temperature>
            <lighting>light-loving</lighting>
            <watering>1000000 ml</watering>
        </growing-tips>
    </tree>
    <bush id="3" name="jasmine">
        <soil>sod-podzolic</soil>
        <origin>Arabia</origin>
        <visual-parametrs>
            <steam-color>green</steam-color>
            <leaf-color>green</leaf-color>
            <size>3 m</size>
        </visual-parametrs>
        <multiplying>cuttings</multiplying>
        <date-of-planting>2021-10-01</date-of-planting>
        <growing-tips>
            <temperature>23 degree</temperature>
            <lighting>not-light-loving</lighting>
            <watering>10000 ml</watering>
        </growing-tips>
    </bush>
</flowers>